{
  "manifest_version": 3,
  "version": "0.0.1",
  "name": "Demo",
  "description": "Demo",
  "icons": {
    "16": "/img/logo-16.png",
    "128": "/img/logo-128.png"
  },
  "background": {
    "service_worker": "/background.js"
  },
  "content_scripts": [
    {
      "world": "MAIN",
      "run_at": "document_start",
      "js": ["/content.js"],
      "matches": ["https://mail.google.com/*"]
    }
  ],
  "host_permissions": ["https://mail.google.com/*"]
}
